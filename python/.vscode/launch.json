{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GoogleCloudStorageDatasource",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/google_cloud_storage/main.py",
            "python": "${workspaceFolder}/.venv/bin/python",
            "cwd": "${workspaceFolder}/examples/google_cloud_storage",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "GEVENT_SUPPORT": "True"
            }
        },
        {
            "name": "Github",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/github/main.py",
            "python": "${workspaceFolder}/.venv/bin/python",
            "cwd": "${workspaceFolder}/examples/github",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "GEVENT_SUPPORT": "True"
            }
        }
    ]
}